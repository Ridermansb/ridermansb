{
  "name": "ridermansb",
  "description": "A personal npm card for ridermansb (@ridermansb)",
  "main": "./src/index.js",
  "bin": {
    "ridermansb": "./bin/index.js"
  },
  "files": [
    "bin/"
  ],
  "scripts": {
    "build": "rimraf bin/* && babel --source-maps -d bin/ src/",
    "start": "npm run build -- --watch",
    "prepublish": "npm run -s build",
    "release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ridermansb/ridermansb.git"
  },
  "keywords": [
    "npm",
    "npx",
    "node",
    "javascript",
    "front-end",
    "npx card",
    "business card"
  ],
  "author": "Riderman de Sousa Barbosa <ridermansb@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ridermansb/ridermansb/issues"
  },
  "homepage": "https://github.com/Ridermansb/ridermansb#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "commitizen": "^4.2.4",
    "core-js": "^3.21.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2"
  },
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "inquirer": "^7.3.2",
    "open": "^7.0.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
